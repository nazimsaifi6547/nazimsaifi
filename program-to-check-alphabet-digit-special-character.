int main(){
    char ch;
    printf("enter any charactor\n");
    scanf("%c",&ch);
    
    if  ( (ch>='a' && ch>='z') || (ch>='A' && ch<='Z')){
        printf(" %c is alphabet ",ch);
        
    }
     else if (ch>='0' || ch<='9'){
        printf("%c is digit ",ch);
        
    }

    
    else {
        printf("%c it is special charactor ",ch);
        
    }
    return 0 ;}
    
