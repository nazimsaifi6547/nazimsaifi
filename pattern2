#include<stdio.h>
int main(int argc, char const *argv[])
{
      int n=5;
      int count=0;
      for(int i=1; i<=n; i++)
      {
        for(int j=1; j<=n; j++)
        {
          if(i==1 || i==n || j==1 || j==n)
          {
            printf("*");
          } 
          else 
          {
            printf("%c",'A' + count);
          } 
        }
        printf("\n");
      }
    return 0;
}
